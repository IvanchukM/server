<!DOCTYPE html>
<head>
    <body>
        <div class="container">
            <h2><%= title %></h2>
            <table class="table table-dark table-striped">
                <thead>
                    <tr>
                        <th>Customer Name</th>
                        <th>Coffee Name</th>
                        <th>Coffee Volume</th>
                        <th>Coffee Price</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(var i = 0; i < orders.length ; i++) { %>
                        <tr>
                            <td><%= orders[i].customer_name %></td>
                            <td><%= orders[i].coffee_name %></td>
                            <td><%= orders[i].coffee_volume %></td>
                            <td><%= orders[i].coffee_price %></td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </body>
</head>